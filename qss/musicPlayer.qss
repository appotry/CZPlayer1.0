/************************************QScrollBar**************************************************/

QScrollBar:vertical
{
    width:15px;
    border:1px solid rgb(156, 170, 193);/*±ß¿òÑÕÉ«*/
    margin:0px,0px,0px,0px;
    padding-top:10px;
    padding-bottom:10px;
}

QScrollBar::handle:vertical
{
    width:15px;
    background:rgb(156, 170, 193);/*¿ØÖÆÌõÑÕÉ«*/
    min-height:15;
    border-radius: 3px;
}

QScrollBar::handle:vertical::disabled
{
    background:rgb(156, 170, 193);
}

QScrollBar::add-line:vertical
{
    height:10px;
    background:rgb(156, 170, 193);
    subcontrol-position:bottom;
}

QScrollBar::sub-line:vertical
{
    height:10px;
    background:rgb(156, 170, 193);
    subcontrol-position:top;
}

/*¹ö¶¯ÌõÉÏÃæ²¿·Ö¹ö¶¯ÌõÏÂÃæ²¿·Ö*/
QScrollBar::add-page:vertical
{
    background: rgb(228, 227, 228);
}

/*¹ö¶¯ÌõÉÏÃæ²¿·Ö*/
QScrollBar::sub-page:vertical
{
    background: rgb(228, 227, 228);
}

QScrollBar::up-arrow:vertical
{
    border-width:1px;
    max-height:15px;
    min-width:15px;
    border-radius: 3px;
}

QScrollBar::down-arrow:vertical
{
        border-width:1px;
        max-height:15px;
        min-width:15px;
        border-radius: 3px;
}

